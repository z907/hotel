<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="DefaultColor">#262626</Color>
    <Brush x:Key="DefaultColorBrush">#262626</Brush>
    <Color x:Key="BitLighterColor"> #2B2B2B</Color>
    <Brush x:Key="BitLighterColorBrush"> #2B2B2B</Brush>
    <Color x:Key="LessLighterColor"> #3B3B3B</Color>
    <Brush x:Key="LessLighterColorBrush"> #3B3B3B</Brush>
    <Color x:Key="LighterColor">#615c5c</Color>
    <Brush x:Key="LighterColorBrush">#615c5c</Brush>
    <Color x:Key="LightColor">#8c8787</Color>
    <Brush x:Key="LightColorBrush">#8c8787</Brush>
    <Color x:Key="VeryLight">#b5b0b0</Color>
    <Brush x:Key="VeryLightBrush">#b5b0b0</Brush>
</ResourceDictionary>